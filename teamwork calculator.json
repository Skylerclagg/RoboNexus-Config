      "fields": [
        { "key": "dropZoneTopCleared",   "label": "Tops Cleared",           "type": "Int" },
        { "key": "greenBeanBags",        "label": "Green Drop Zone Bags",  "type": "Int" },
        { "key": "blueBeanBags",         "label": "Blue Drop Zone Bags",   "type": "Int" },
        { "key": "neutralBalls",         "label": "Neutral Balls",         "type": "Int" },
        { "key": "greenBalls",           "label": "Green Balls",           "type": "Int" },
        { "key": "blueBalls",            "label": "Blue Balls",            "type": "Int" },
        {
          "key": "redDroneSelection",
          "label": "Red Drone Landing",
          "type": "Enum",
          "options": ["None", "Small Cube", "Large Cube", "Landing Pad", "Bullseye"],
          "values":  [   0   ,    25     ,     15      ,      15      ,     25    ]
        },
        {
          "key": "blueDroneSelection",
          "label": "Blue Drone Landing",
          "type": "Enum",
          "options": ["None", "Small Cube", "Large Cube", "Landing Pad", "Bullseye"],
          "values":  [   0   ,    25     ,     15      ,      15      ,     25    ]
        }
      ],
      "formula": "\
dropZoneTopCleared + neutralBalls + greenBeanBags + blueBeanBags \
+ greenBalls + blueBalls \
